# Jira Configuration
JIRA_SERVER="https://your-domain.atlassian.net"
JIRA_PAT="your_jira_personal_access_token"

# GitLab Configuration
GITLAB_SERVER="https://gitlab.com"
GITLAB_PRIVATE_TOKEN="your_gitlab_private_access_token"

# AI Model Configuration
# Choose your AI service provider: 'gemini' or 'openai'
AI_SERVICE_PROVIDER="gemini"

# Configuration for Google Gemini (if AI_SERVICE_PROVIDER is 'gemini')
GEMINI_API_KEY="your_gemini_api_key"

# Configuration for OpenAI (if AI_SERVICE_PROVIDER is 'openai')
OPENAI_API_KEY="your_openai_api_key"